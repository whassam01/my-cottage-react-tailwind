  "scripts": {
    "build:tailwind": "tailwindcss build src/tailwind.css -o src/tailwind.output.css",
    "prestart": "npm run build:tailwind",
    "prebuild": "npm run build:tailwind",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "predeploy": "NODE_OPTIONS=--max_old_space_size=4096 npm run build",
    "deploy": "aws s3 sync build/ s3://cottage-web --delete",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id EJJPFK40P21MA --paths '/*'",
    "eject": "react-scripts eject",
    "format": "npx prettier --write \"**/*.{js,jsx,json,md,html}\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
